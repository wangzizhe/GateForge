{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://gateforge.dev/schemas/planner_output.schema.json",
  "title": "GateForge Planner Output v0",
  "type": "object",
  "required": ["intent", "overrides"],
  "additionalProperties": false,
  "properties": {
    "intent": {
      "type": "string",
      "enum": [
        "demo_mock_pass",
        "demo_openmodelica_pass",
        "medium_openmodelica_pass",
        "runtime_regress_low_risk",
        "runtime_regress_high_risk"
      ]
    },
    "proposal_id": {
      "type": ["string", "null"],
      "minLength": 1
    },
    "overrides": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "risk_level": { "type": "string", "enum": ["low", "medium", "high"] },
        "change_summary": { "type": "string", "minLength": 1 },
        "checkers": { "type": "array", "items": { "type": "string" } },
        "checker_config": { "type": "object" },
        "change_set_path": { "type": "string", "minLength": 1 }
      }
    },
    "change_plan": {
      "type": "object"
    },
    "change_set_draft": {
      "type": "object"
    },
    "planner": {
      "type": "string",
      "minLength": 1
    },
    "planner_inputs": {
      "type": "object"
    },
    "context": {
      "type": "object"
    },
    "raw_response": {
      "type": "object"
    }
  }
}
